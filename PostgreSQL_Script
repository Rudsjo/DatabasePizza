--

CREATE FUNCTION public.updateemployeebyid("_UserID" integer, "_Password" text, "_Role" text) RETURNS void
    LANGUAGE plpgsql
    AS $$
BEGIN
	UPDATE "Employees"
	SET 
	"Password" = "_Password",
	"Role" = "_Role"
	WHERE "Employees"."UserID" = "_UserID";
END;
$$;



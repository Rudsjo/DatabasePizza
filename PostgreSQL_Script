		DELETE FROM "CashierOrdersRelationship" 
		WHERE "Employees"."UserID" = "_UserID" 
			AND "Orders"."OrderID" = "_OrderID";

		UPDATE "Orders"
		SET "Orders"."Status" = 0
		WHERE "Orders"."OrderID" = "_OrderID";
END;
$$;


